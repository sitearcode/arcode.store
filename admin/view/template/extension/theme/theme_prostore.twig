                          <label class="btn btn-default btn-off {{ theme_prostore_schema == '0' ? 'active' : '' }}">
                            <input value="0" type="radio" name="theme_prostore_schema" id="theme_prostore_schema-0" autocomplete="off" {{ theme_prostore_schema == '0' ? 'checked' : '' }}>{{ text_off }}
