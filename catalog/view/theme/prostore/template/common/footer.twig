  {% endif %}
  {% if telephone %}
  "telephone": "{{ telephone }}",
  {% endif %}
  "email": "{{ shop_email }}",
  "priceRange": "{{ currency }}",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "{{ address_schema }}",
