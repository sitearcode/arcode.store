  },
  {% if geocode %}
  "geo": {
    "@type": "GeoCoordinates",
    "latitude": "{{ latitude }}",
    "longitude": "{{ longitude }}"
  },
  {% endif %}
  {% if meta_description %}
  "description": "{{ meta_description }}",
  {% endif %}
  "url": "{{ home }}"
}
</script>
<!-- Schema Org :: End-->
