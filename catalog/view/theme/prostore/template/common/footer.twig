							<a class="footer__call-link" rel="nofollow" href="tel:{{ prostore_phone[language_id]|replace({' ': '', '-': '', '(': '', ')': ''}) }}"> {{ prostore_phone[language_id] }}</a>
						</li>
						{% endfor %}
					</ul>
				</div>
			</div>
